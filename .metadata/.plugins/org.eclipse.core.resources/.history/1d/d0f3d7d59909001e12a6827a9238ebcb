package com.fin.proj.board.model.vo;

import java.sql.Date;

public class Reply {
	
	private int reNo;
	private int reWriter;
	private int boardNo;
	private String reContent;
	private Date reCreateDate;
	private char reStatus;
	
	public Reply () {}

	public Reply(int reNo, int reWriter, int boardNo, String reContent, Date reCreateDate, char reStatus) {
		super();
		this.reNo = reNo;
		this.reWriter = reWriter;
		this.boardNo = boardNo;
		this.reContent = reContent;
		this.reCreateDate = reCreateDate;
		this.reStatus = reStatus;
	}

	public int getReNo() {
		return reNo;
	}

	public void setReNo(int reNo) {
		this.reNo = reNo;
	}

	public int getReWriter() {
		return reWriter;
	}

	public void setReWriter(int reWriter) {
		this.reWriter = reWriter;
	}

	public int getBoardNo() {
		return boardNo;
	}

	public void setBoardNo(int boardNo) {
		this.boardNo = boardNo;
	}

	public String getReContent() {
		return reContent;
	}

	public void setReContent(String reContent) {
		this.reContent = reContent;
	}

	public Date getReCreateDate() {
		return reCreateDate;
	}

	public void setReCreateDate(Date reCreateDate) {
		this.reCreateDate = reCreateDate;
	}

	public char getReStatus() {
		return reStatus;
	}

	public void setReStatus(char reStatus) {
		this.reStatus = reStatus;
	}

	@Override
	public String toString() {
		return "Reply [reNo=" + reNo + ", reWriter=" + reWriter + ", boardNo=" + boardNo + ", reContent=" + reContent
				+ ", reCreateDate=" + reCreateDate + ", reStatus=" + reStatus + "]";
	}
}
