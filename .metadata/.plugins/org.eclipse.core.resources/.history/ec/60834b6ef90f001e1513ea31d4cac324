package com.fin.proj.support.model.vo;

public class SupportDetail {
	private int supportNo;
	private String supportDetailContent;
	private int supportDetailAmount;
	private int supportDetailNo;
	
	public SupportDetail() {}

	public SupportDetail(int supportNo, String supportDetailContent, int supportDetailAmount, int supportDetailNo) {
		super();
		this.supportNo = supportNo;
		this.supportDetailContent = supportDetailContent;
		this.supportDetailAmount = supportDetailAmount;
		this.supportDetailNo = supportDetailNo;
	}

	public int getSupportNo() {
		return supportNo;
	}

	public void setSupportNo(int supportNo) {
		this.supportNo = supportNo;
	}

	public String getSupportDetailContent() {
		return supportDetailContent;
	}

	public void setSupportDetailContent(String supportDetailContent) {
		this.supportDetailContent = supportDetailContent;
	}

	public int getSupportDetailAmount() {
		return supportDetailAmount;
	}

	public void setSupportDetailAmount(int supportDetailAmount) {
		this.supportDetailAmount = supportDetailAmount;
	}

	public int getSupportDetailNo() {
		return supportDetailNo;
	}

	public void setSupportDetailNo(int supportDetailNo) {
		this.supportDetailNo = supportDetailNo;
	}

	@Override
	public String toString() {
		return "SupportDetail [supportNo=" + supportNo + ", supportDetailContent=" + supportDetailContent
				+ ", supportDetailAmount=" + supportDetailAmount + ", supportDetailNo=" + supportDetailNo + "]";
	}
	
	
	
	
	
}
